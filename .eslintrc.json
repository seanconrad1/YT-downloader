{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": [
      "eslint-plugin-html"
    ],
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },

    "rules": {
      "no-console":"off",
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
}
